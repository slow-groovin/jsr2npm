{
  "name": "jsr-to-npm",
  "version": "0.3.1",
  "description": "a cli/lib for mirror jsr package to npm locally",
  "author": {
    "name": "slow-groovin",
    "url": "https://github.com/slow-groovin"
  },
  "homepage": "https://github.com/slow-groovin/jsr-to-npm",
  "keywords": [
    "jsr",
    "npm",
    "registry",
    "mirror"
  ],
  "type": "module",
  "private": true,
  "exports": "./src/lib.ts",
  "bin": "./dist/cli.js",
  "scripts": {
    "dev": " bun run src/index.ts @slow-groovin/moonshot-web-ai-provider --log-level debug --skip-check --dry-run",
    "test": "bun test",
    "build": "bun run build.ts",
    "prepublish": "bun test && bun bun build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.14.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@types/shelljs": "^0.8.15",
    "commander": "^13.1.0",
    "consola": "^3.4.2",
    "fetch-pkg": "^1.0.0",
    "nrd": "^1.1.3",
    "publint": "^0.3.12",
    "semver": "^7.7.1",
    "shelljs": "^0.9.2",
    "tar": "^7.4.3",
    "type-fest": "^4.40.0"
  }
}
